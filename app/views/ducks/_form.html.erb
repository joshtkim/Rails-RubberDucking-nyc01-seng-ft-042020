
<%= form_for @duck do |f| %>
    <%= f.label :name%>
    <%= f.text_field :name%>

    <%= f.label :description%>
    <%= f.text_field :description%>

    <%= f.label :student_id%>
    <%= f.collection_select :student_id, Student.all, :id, :name%>
    <%# key, array, what you send back to the server, what you send to the user%>

    <%= f.submit%>

<% end %>